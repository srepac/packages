#!/usr/bin/expect
# Created by @srepac to update-htpasswd 
# Usage:    $0 <username> <password>
# Filename: update-htpasswd.exp
#
set username [lindex $argv 0]
set password [lindex $argv 1]

eval spawn kvmd-htpasswd set $username
expect "Password:"
send "$password\r"
expect "Repeat:"
send "$password\r"
interact
